function buyItem()
{
	if(!isHolding)
	{
		return;
	}
	else if(itemSelected != null)
	{
		// call to ajax as it is async
		retrieveMoney();
	}
	else
	{
		writeNotificationToScreen("You need to select an item to buy before buying something!", 0);
	}
}
function singlePressBuyItem()
{
        if(itemSelected != null)
        {
                // call to ajax as it is async
                retrieveMoney();
        }
        else
        {
                writeNotificationToScreen("You need to select an item to buy before buying something!", 0);
        }
}
